{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Generated schema for Root",
    "type": "object",
    "properties": {
        "success": {
            "type": "boolean"
        },
        "deck_id": {
            "type": "string"
        },
        "cards": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "code": {
                        "type": "string"
                    },
                    "image": {
                        "type": "string"
                    },
                    "images": {
                        "type": "object",
                        "properties": {
                            "svg": {
                                "type": "string"
                            },
                            "png": {
                                "type": "string"
                            }
                        },
                        "required": [
                            "svg",
                            "png"
                        ]
                    },
                    "value": {
                        "type": "string"
                    },
                    "suit": {
                        "type": "string"
                    }
                },
                "required": [
                    "code",
                    "image",
                    "images",
                    "value",
                    "suit"
                ]
            }
        },
        "remaining": {
            "type": "number"
        }
    },
    "required": [
        "success",
        "deck_id",
        "cards",
        "remaining"
    ]
}
